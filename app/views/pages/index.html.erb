<script>
				
$(document).ready(function() {
	$('a[href*=#]').bind("click", function(event) {
		event.preventDefault();
		var ziel = $(this).attr("href");

		$('html,body').animate({
			scrollTop: $(ziel).offset().top
		}, 700 , function (){location.hash = ziel;});
         });
	scroll();
	$( "#submit" ).click(function() {
  		emailjs.send("default_service","template_nNExiXcH",{reply_to: $('#email').val(), from_name: $('#name').val(), to_name: $('#name').val(), message_html: $('#ebody').val() });
  		$('#name').val("");
  		$('#email').val("");
  		$('#ebody').val("");	
	});

});

function scroll() {
    setTimeout(function(){
        window.scrollTo(0,0);
    }, 2000);
}

</script>
